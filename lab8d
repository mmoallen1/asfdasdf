void getscore()
{
    double score[4];
    for (int i=0; i<4; i++)
    {
        cout<<"Enter your test score:"<<endl;
        cin>>score[i];
        if (score[i]<0 || score[i]>100)
            cout<<"Invalid choice"<<endl;
        else
            cout<<"it works"<<endl;
    }
}

